<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Article.
    -->
    <changeSet id="20201006130745-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="categorie_id"
                                 baseTableName="article"
                                 constraintName="fk_article_categorie_id"
                                 referencedColumnNames="id"
                                 referencedTableName="categorie"/>

        <addForeignKeyConstraint baseColumnNames="auteur_id"
                                 baseTableName="article"
                                 constraintName="fk_article_auteur_id"
                                 referencedColumnNames="id"
                                 referencedTableName="auteur"/>

    </changeSet>
</databaseChangeLog>
